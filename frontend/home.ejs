<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
    
<% if(hotel!=null) { %>
    <table>
    <tr>
        <th>Route</th>
        <th>origin </th>
        <th>Destination</th>
        <th>EstimatedTimeOfArrival </th>
        <th>Date </th>
        <th>Time</th>
    </tr>
    <% hotel.forEach(function(hotel){ %>
    <tr>
        <td><%= hotel.name %></td>
        <td><%= hotel.price %></td>
        <td><%= hotel.city%></td>
        <td><%= hotel.address %></td>
        <td><%= hotel.contact %></td>
        
    
    </tr>
    <% }) %>
    </table>
    <% } %>




</body>
</html>